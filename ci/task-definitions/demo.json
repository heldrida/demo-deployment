{
    "family": "TASK_NAME",
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "sumologic",
                "options": {
                    "sumo-url": "SUMO_URL",
                    "sumo-source-category": "ENVIRONMENT/TASK_NAME"
                }
            },

            "environment": [],
            "name": "TASK_NAME",
            "image": "DOCKER_IMAGE",
            "cpu": 100,
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 80
                }
            ],
            "essential": true
        }
    ]
}
